{
    "css": {
        "empty_zero": {
            "f": "gi",
            "p": "(\\-?\\b(0|0\\.0)(em|ex|%|px|cm|mm|in|pt|pc|ch|rem|vh|vw|vmin|vmax)|\\-?\\b0\\.0\\b)",
            "r": "0"
        },
        "lead_zeros": {
            "f": "g",
            "p": "([^\\d])(\\.\\d+)",
            "r": "$10$2"
        },
        "lowercase_hex": {
            "f": "gi",
            "p": "#[a-f0-9]{3,6}"
        },
        "prefixes": {
            "f": "gi",
            "p": "(\\s+)?\\-(moz|o|webkit|ms|khtml)\\-(?!font-smoothing|osx|print|backface).+?;",
            "r": ""
        }
    },
    "html": {
        "post": {
            "f": "gi",
            "p": "\\$:post\\{\\$?[\\w\\d-_]+\\}",
            "r": ""
        },
        "pre": {
            "f": "gi",
            "p": "\\$:pre\\{\\$?[\\w\\d-_]+\\}",
            "r": ""
        }
    }
}
